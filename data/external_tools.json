{
  "vc": {
    "check-repo-clean": "git status --porcelain",
    "check-staged-files": "git diff --cached --name-only",
    "commit-and-push": "git add . && git commit -m $message && git push",
    "push-with-tags": "git push && git push --tags",
    "check-available": "git --version",
    "check-is-repo": "git rev-parse --git-dir",
    "preview-clean": "git clean -xdn",
    "clean-untracked": "git clean -xdf"
  },
  "package-manager": {
    "update-dependencies": "update2latest . dependencies && npm update",
    "increment-patch-version": "npm version patch",
    "publish-package": "npm publish",
    "get-published-version": "npm view $package version",
    "pack-and-extract": "npm pack $path && tar -xzf $file",
    "install-dependencies": "npm install"
  },
  "visual-diff": {
    "compare-directories": "diff -r $dir1 $dir2",
    "launch": "bcompare $dir1 $dir2",
    "check-available": "which bcompare"
  },
  "graph": {
    "render": "dot -Tsvg $input -o $output",
    "check-available": "which dot"
  },
  "shell": {
    "open-file": "xdg-open $file"
  }
}
